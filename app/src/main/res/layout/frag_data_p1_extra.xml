<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:clickable="true">

    <include
        android:id="@+id/top_menu_back"
        layout="@layout/top_menu_back"
        android:layout_alignParentTop="true"
        android:layout_width="match_parent"
        android:layout_height="52dp" />

 <!--   <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/swipe_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent">-->

    <RelativeLayout
        android:id="@+id/bottom_message_layout"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_alignParentBottom="true">

        <TextView
            android:id="@+id/data_f5_ua_message"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/data_f5_ua_message"
            android:layout_centerInParent="true"
            android:textAlignment="center" />
    </RelativeLayout>

    <ExpandableListView
        android:id="@+id/payment_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="20dp"
        android:layout_below="@+id/top_menu_back"
        android:layout_above="@+id/bottom_message_layout"/>

  <!--  </android.support.v4.widget.SwipeRefreshLayout>-->


</RelativeLayout>